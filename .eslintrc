{
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "node": true
  },
  "settings": {
    "ecmascript": 5,
    "jsx": true
  },
  "plugins": ["react"],
  "rules": {
    "comma-style": [2, "last"],
    "curly": [2, "all"],
    "dot-notation": [
      2,
      {
        "allowKeywords": true
      }
    ],
    "eqeqeq": [2, "allow-null"],
    "strict": [2, "global"],
    "new-cap": 2,
    "no-bitwise": 2,
    "no-cond-assign": [2, "except-parens"],
    "no-debugger": 2,
    "no-empty": 2,
    "no-eval": 2,
    "no-extend-native": 2,
    "no-extra-parens": 2,
    "no-irregular-whitespace": 2,
    "no-loop-func": 2,
    "no-new": 2,
    "no-plusplus": 2,
    "no-proto": 2,
    "no-shadow": 2,
    "no-undef": 2,
    "no-unused-vars": 2,
    "no-with": 2,
    "quotes": [2, "single"],
    "semi": [2, "always"]
  }
}
